{
  "applicationId": "com.mediata.app",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/mediata-ui/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log",
    "filesystem.*",
    "storage.*"
  ],
  "modes": {
    "window": {
      "title": "mediata",
      "width": 800,
      "height": 500,
      "minWidth": 650,
      "minHeight": 450,
      "enableInspector": true,
      "icon": "/mediata-ui/public/app-icon.png"
    }
  },
  "cli": {
    "binaryName": "mediata",
    "resourcesPath": "/mediata-ui/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.13.0",
    "clientVersion": "3.8.2",
    "frontendLibrary": {
      "patchFile": "/mediata-ui/index.html",
      "devUrl": "http://localhost:5173"
    }
  }
}