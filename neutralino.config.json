{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/mediata-ui/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "mediata",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/mediata-ui/public/app-icon.png"
    }
  },
  "cli": {
    "binaryName": "mediata",
    "resourcesPath": "/mediata-ui/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.13.0",
    "clientVersion": "3.8.2"
  }
}